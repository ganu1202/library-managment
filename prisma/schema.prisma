generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt      DateTime @db.Date
  email          String   @unique
  name           String
  password       String
  universityFile String
  universityId   String
}

model User_Details {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}
